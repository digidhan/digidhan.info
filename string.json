function fill_template() {

	var str = document.getElementById("wholebody").innerHTML ;
	for(var key in string_json) {
		str = str.replace("#{" + key + "}", string_json[key]);
	}	
	document.getElementById("wholebody").innerHTML  = str;
	var str = document.head.innerHTML ;
	for(var key in string_json) {
		str = str.replace("#{" + key + "}", string_json[key]);
	}	
	document.head.innerHTML  = str;
	
}
window.onload = fill_template;

var string_json = {
    my: "Am",
    name: "I",
    is: "<a href='#'>awesome</a>",
    a: "#{b}",
    b: "c",
    c: "?"
};
